<h1>Listing data_set_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_set_types.each do |data_set_type| %>
      <tr>
        <td><%= data_set_type.name %></td>
        <td><%= link_to 'Show', data_set_type %></td>
        <td><%= link_to 'Edit', edit_data_set_type_path(data_set_type) %></td>
        <td><%= link_to 'Destroy', data_set_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data set type', new_data_set_type_path %>

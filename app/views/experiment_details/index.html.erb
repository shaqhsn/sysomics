<h1>Listing experiment_details</h1>

<table>
  <thead>
    <tr>
      <th>Sample</th>
      <th>Platform</th>
      <th>Protocol</th>
      <th>Disease</th>
      <th>Experiment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiment_details.each do |experiment_detail| %>
      <tr>
        <td><%= experiment_detail.sample_id %></td>
        <td><%= experiment_detail.platform_id %></td>
        <td><%= experiment_detail.protocol_id %></td>
        <td><%= experiment_detail.disease_id %></td>
        <td><%= experiment_detail.experiment_id %></td>
        <td><%= link_to 'Show', experiment_detail %></td>
        <td><%= link_to 'Edit', edit_experiment_detail_path(experiment_detail) %></td>
        <td><%= link_to 'Destroy', experiment_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experiment detail', new_experiment_detail_path %>
